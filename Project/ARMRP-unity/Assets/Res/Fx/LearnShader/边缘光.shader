				float4 appendResult19 = (float4(_x , _y , 0.0 , 0.0));
				float2 uv_TextureSample0 = IN.ase_texcoord3.xy * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
				float2 panner16 = ( 1.0 * _Time.y * appendResult19.xy + uv_TextureSample0);
