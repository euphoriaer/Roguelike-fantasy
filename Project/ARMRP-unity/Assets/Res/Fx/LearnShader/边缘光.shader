				float dotResult8 = dot( ase_worldNormal , ase_worldViewDir );
				float temp_output_20_0 = ( step( tex2D( _TextureSample0, panner16 ).r , _edgePow ) * saturate( pow( ( 1.0 - dotResult8 ) , _FresnelPow ) ) );
				
				float3 BakedAlbedo = 0;
