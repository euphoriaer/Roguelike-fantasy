				float4 tex2DNode5 = tex2D( _TextureSample0, uv_TextureSample0 );
				float temp_output_10_0 = step( _Float0 , ( tex2DNode5.r + length ) );
				float temp_output_13_0 = ( temp_output_10_0 - step( _Float0 , tex2DNode5.r ) );
				float4 lerpResult18 = lerp( tex2DNode6 , ( tex2DNode6.a * temp_output_13_0 * _Color0 ) , temp_output_13_0);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = (lerpResult18).rgb;
				float Alpha = ( (lerpResult18).a * temp_output_10_0 );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

